<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Button layoutX="30.0" layoutY="226.0" mnemonicParsing="false" onAction="#Login" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TextField fx:id="userName" layoutX="9.0" layoutY="99.0" prefHeight="39.0" prefWidth="255.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordField" layoutX="9.0" layoutY="150.0" prefHeight="39.0" prefWidth="255.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblStatus" alignment="CENTER" contentDisplay="CENTER" layoutY="42.0" prefHeight="17.0" prefWidth="275.0" text="Welcome to Car Rental System" textFill="#e40707">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="114.0" layoutY="228.0" mnemonicParsing="false" onAction="#createAccount" text="Create Account">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
